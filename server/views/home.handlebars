{{>header}}

<main class="">
  <ul class="product-list col-xs-12 col-sm-12 col-md-10 col-lg-10">
    {{#each products}}
    <li class="col-xs-6 col-sm-4 col-md-4 col-lg-3">
      <img class="img-responsive" alt="{{this.image.0.alt}}" src="{{this.images.0.url}}">
      <h4>{{ this.name }}</h4>
      <p>{{ this.description }}</p>
    </li>
    {{/each}}
  </ul>
</main>
